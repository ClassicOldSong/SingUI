{
  "name": "singui",
  "version": "0.2.4-alpha.2",
  "description": "The next-gen, no compile/transpile needed, self-contained JS UI library",
  "main": "dist/main.min.js",
  "module": "src/main.mjs",
  "scripts": {
    "build": "rollup -c ./rollup.esbuild.config.js",
    "release": "npx np"
  },
  "exports": {
    "module": "dist/main.min.js",
    "script": "dist/main.umd.min.js",
    "require": "dist/main.min.cjs",
    "node": "src/main.mjs",
    "default": "dist/main.js"
  },
  "files": [
    "src/**/*",
    "dist/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ClassicOldSong/SingUI.git"
  },
  "keywords": [
    "singui"
  ],
  "author": "Yukino Song",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ClassicOldSong/SingUI/issues"
  },
  "homepage": "https://github.com/ClassicOldSong/SingUI",
  "devDependencies": {
    "esbuild": "^0.13.14",
    "rollup": "^2.60.0",
    "rollup-plugin-esbuild": "^4.7.1",
    "rollup-plugin-terser": "^7.0.2"
  }
}
